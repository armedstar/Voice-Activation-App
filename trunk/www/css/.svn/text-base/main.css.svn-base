    padding-left:3px;
    margin:0;
    width:450px;
}

#castFilmography #details{
    width:100%;
    background-color:#000000;
    font-family:Museo;
    color:#ffffff;
    background: rgba(0, 0, 0, 0.6);
    padding:10px 10px 10px 150px;
    height:65px;
    margin-top:-9px;
}


/*---Right Bubble---*/
#rightBubble{
    width:343px;
    position:absolute;
    bottom:350px;
    left:540px;
    z-index:9999;
}

#rightBubble #rBubbleMain{
    background-image:url('../images/rBubbleTile.png');
    background-repeat: repeat-y;
    padding:0 20px;
    font-family: Museo, sans-serif;
    font-size:23px;
    padding-left:30px;
    padding-top:20px;
}

#rightBubble #rBubbleTop{
    position:relative;
    top:0px;
    background-image:url('../images/rBubbleTop.png'); 
    background-repeat:no-repeat;
    height:18px;
}
#rightBubble #rBubbleBottom{
    background-image:url('../images/rBubbleBottom.png');
    background-repeat:no-repeat;
    position:relative;
    bottom:0px;
    height: 33px;
    clear:both;
}

/*----Left Bubble---*/
#leftBubble{
    width:343px;
    position:absolute;
    bottom:470px;
    left:100px;
    z-index:9999;
}
#leftBubble #lBubbleMain{
    background-image:url('../images/lBubbleTile.png'); 
    background-repeat:repeat-y;
    padding-left:30px;
    padding-right:30px;
    padding-top:5px;
    font-family: Museo, sans-serif;
    font-size:23px;
}

#leftBubble #lBubbleTop{
    position:relative;
    top:0px;
    background-image:url('../images/lBubbleTop.png');
    background-repeat:no-repeat;
    height:18px;
}
#leftBubble #lBubbleBottom{
    background-image:url('../images/lBubbleBottom.png');
    background-repeat:no-repeat;
    position:relative;
    bottom:0px;
    height: 33px;
    clear:both;
}


