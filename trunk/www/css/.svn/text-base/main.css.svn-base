    margin:0 auto;
    width:775px;
}

#castOverview p {
    font-family:Museo;
    font-size:18px;
    color:#ffffff;
    padding: 15px 0;
}

#loadingAnimation {
    vertical-align:text-bottom;
}

ul {
    font-family:Museo;
    font-size:18px;
    color:#ffffff;
}

#castOverview #details {
    width:736px;
    background-color:#000000;
    font-family:Museo;
    color:#ffffff;
    background: rgba(0, 0, 0, 0.6);
    padding:20px;
    height:65px;
    
}

#castFilmography table{
    padding-left:150px;
    width:800px;
}



#detailsNav{
    background-image:url('../images/detailsNavTile.png'); 
    background-repeat:repeat-x;
    margin:0 auto;
    padding:10px;
    width:1024px;
    text-align:center;
}

#details h2{
    margin:0;
    padding-top:13px;
}

#details p{
    margin:0;
    padding:0;
}

.celebImgSm{
    float:left;
    padding:10px;
    max-width:94px;
    max-height:57px;
}

#onDemandContentTable, #favoritesContentTable {
    border-collapse:separate;
    border-spacing:8px 8px;
}

#favoritesContent table td, #onDemandContent table td{
    padding:0;
}

#queueContent table{
    width:800px;
}

#creditsDivider {
    height:7px;
    background: url(../images/divider.png);
}

#queueDivider {
    height:7px;
    background: url(../images/divider.png) no-repeat center;
}

#queueContent table td, #castFilmography table td{
