#notifications{
    position:absolute;
    top:100px;
    left:0px;
    width:1024px;
    height:688px;
    
}

#favoritesContent, #onDemandContent, #queueContent {
    position:absolute;
    top:100px;
    left:2000px;
    width:1024px;
    height:688px;
    margin:0 auto;
    opacity:1;
    overflow-y:scroll;
    -webkit-transition: left .5s ease-out;
}

#castOverview, #castFilmography {
    position:absolute;
    top:57px;
    left:2000px;
    overflow-y:scroll;
    -webkit-transition: left .5s ease-out;
    height:688px;
}


#onTV{
    position:absolute;
    top:100px;
    left:160px;
    width:800px;
    height:688px;
    opacity:0;
}

.onTvImg{
    position:relative;
    left:175px;
    top:50px;
}


#onDemandContent.animateIn, #favoritesContent.animateIn {
    left:85px;
}

#queueContent.animateIn {
    left:130px;
}

#castOverview.animateIn, #castFilmography.animateIn {
    left:0px;
}

#favoritesContent.animateOut, #overviewContent.animateOut, #queueContent.animateOut, #castOverview.animateOut, #castFilmography.animateOut {
    left:-2000px;
    display:none;
}

#onDemandContent.fadeOut, #onTV.fadeOut, #favoritesContent.fadeOut, #queueContent.fadeOut, #castOverview.fadeOut, #castFilmography.fadeOut {
    opacity:0;
    -webkit-transition: opacity .5s ease-out;
}

#onTV.fadeIn{
    opacity:1;
    -webkit-transition: opacity .5s ease-in;
}

#featurePhoto {
    background-image:url('../images/overviewPlaceholder.png');
    background-repeat:no-repeat;
    text-align:center;
}

#favoritesContent h2, #onDemandContent h2, #queueContent h2, #castOverview h2, #castFilmography h2 {
    font-family:Museo;
    font-size:22px;
    color:#ffffff;
    padding:0;
    
}

#castFilmography h2 {
    margin-left:0;
}

#programPlaceholder {
    background-image:url('../images/programPlaceholder.png');
    background-repeat:no-repeat;
    width:135px;
    height:135px;
    text-align:center;
    vertical-align:middle;
}

#programPlaceholderQueue {
    background-image:url('../images/programPlaceholder.png');
    background-repeat:no-repeat;
    width:135px;
    height:135px;
}

#programIcon {
    max-height:133px;
    max-width:130px;
}

#queueContent table td h2, #castFilmography table td h2{
    padding:0 !important;
    margin-top:-2px;
    
}
#queueContent table td h2 span, #castFilmography table td h2 span{
    padding:2px 0 0 3px !important;
    margin:0 !important;
}
#progressBar{
    position:absolute;
    left:2px;
    top:183px !important;
    z-index:999;
}

#recordProgram{
    position:absolute;
    left:567px;
    top:165px;
    z-index:9999;
}

